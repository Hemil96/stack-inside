#!/home/hemil/Desktop/python/practice/click/venv/bin/python2
# EASY-INSTALL-ENTRY-SCRIPT: 'Stack-Inside','console_scripts','stackinside'
__requires__ = 'Stack-Inside'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Stack-Inside', 'console_scripts', 'stackinside')()
    )
